{
    "manifest_version": 3,
    "name": "Azure DevOps Planning Poker",
    "author": "Scott Clayton",
    "version": "1.0",
    "description": "Easily vote on the effort for a product backlog item in DevOps.",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png"
    },
    "content_scripts": [
        {
            "js": [
                "scripts/content.js"
            ],
            "matches": [
                "https://dev.azure.com/*"
            ]
        }
    ]
}